package richRedstoneOre;

import net.minecraftforge.fml.common.registry.GameRegistry;

public class StartupCommon {

	public static RichRedstoneOre blockRichRedstoneOre;
	
	
	
	public static void preInitCommon() {
		
		blockRichRedstoneOre = (RichRedstoneOre)(new RichRedstoneOre().setUnlocalizedName("richRedstoneOre"));
		GameRegistry.registerBlock(blockRichRedstoneOre, "richRedstoneOre");
		
		
	}
	
	
	public static void initCommon() {
		
	}
	
	public static void postInitCommon() {
		
	}
	
	
	
	
	
	
}
